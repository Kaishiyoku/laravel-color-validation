{
    "name": "kaishiyoku/laravel-color-validation",
    "description": "Laravel 5 color validation rules",
    "keywords": [
        "laravel",
        "validator",
        "color"
    ],
    "homepage": "https://github.com/Kaishiyoku/laravel-color-validation",
    "license": "MIT",  
    "authors": [
        {
            "name": "Kaishiyoku",
            "email": "dev@andreas-wiedel.de"
        }
    ],
    "autoload":{
        "psr-4":{
            "Kaishiyoku\\":"src/Kaishiyoku"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaishiyoku\\Validation\\Color\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",  
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.4|6.*",
        "illuminate/validation": "^5.4|6.*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.5|8.*"
    },
    "suggest": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaishiyoku\\Validation\\Color\\ServiceProvider"
            ]
        }
    }
}
