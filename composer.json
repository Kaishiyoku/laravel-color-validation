{
    "name": "kaishiyoku/laravel-color-validation",
    "description": "Laravel  color validation rules",
    "keywords": [
        "laravel",
        "validation rules",
        "validator",
        "color"
    ],
    "homepage": "https://github.com/Kaishiyoku/laravel-color-validation",
    "license": "MIT",
    "authors": [
        {
            "name": "Kaishiyoku",
            "email": "dev@andreas-wiedel.de",
            "homepage": "https://andreas-wiedel.de",
            "role": "Maintainer"
        }
    ],
    "autoload":{
        "psr-4":{
            "Kaishiyoku\\":"src/Kaishiyoku"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaishiyoku\\Validation\\Color\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/validation": "^6.0|^7.0|^8.0",
        "spatie/regex": "^1.4|^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaishiyoku\\Validation\\Color\\ServiceProvider"
            ]
        }
    }
}
